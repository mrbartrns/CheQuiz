{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "browsers": ["last 2 versions", ">= 5% in KR"] } }
    ],
    ["@babel/preset-react", {"runtime": "automatic", "importSource": "@emotion/react"}],
    "@babel/typescript"
  ],
  "plugins": [
    "@emotion",
    ["@babel/plugin-transform-runtime", {
    "corejs": 3
    }
    ]
  ]
}
